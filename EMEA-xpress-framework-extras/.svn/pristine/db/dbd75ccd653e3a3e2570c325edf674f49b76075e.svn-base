package com.fnis.xes.framework.filter.converters;

public enum AcctKeysFormatType {
    // AcctId
    ID,
    // AcctIdent/[AcctIdentType=Profile]
    IDENT_VALUE_PROFILE,
    // AcctIdent/[AcctIdentType=AgreementID]
    IDENT_VALUE_AGREEMENT_ID,
    // AcctIdent/[AcctIdentType=SortCodeAcctNo]
    SORTCODEACCTNO,
    // AcctIdent/[AcctIdentType=SortCodeProfileCid]
    SORTCODEPROFILECID,
    // AcctIdent/[AcctIdentType=Refnum]
    REFNUM,
    // meta type - describes canonical form
    CANONICAL,
    // meta type - groups recognized but non-modifiable types
    OTHER,
    // meta type - other groups which are not be handled (usually generate TransformError Exception)
    UNKNOWN
}
