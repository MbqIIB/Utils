<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>com.fisglobal.xpress.emea</groupId>
        <artifactId>master-settings-pom</artifactId>
        <version>1.4</version>
    </parent>

    <name>EMEA CSF connector</name>
    <artifactId>emea-xpress-connector-csf</artifactId>
    <version>1.0-SNAPSHOT</version>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <spring-ws-core.version>2.1.4.RELEASE</spring-ws-core.version>
        <xpress.testhelper.version>1.4</xpress.testhelper.version>
        <CustomerCorrespondenceCommonClasses.version>ATOM-TRUNK-SNAPSHOT</CustomerCorrespondenceCommonClasses.version>
        <spring-test.version>3.2.6.RELEASE</spring-test.version>
        <testng.version>6.8.7</testng.version>
        <commons-codec.version>1.9</commons-codec.version>
        <log4j.version>1.2.15</log4j.version>
        <xpress.testhelper.version>1.4</xpress.testhelper.version>
        <axiom-impl.version>1.2.12</axiom-impl.version>
        <ECCoreBase.version>1.0.0</ECCoreBase.version>
        <jdom.version>2.0.1</jdom.version>
    </properties>

    <dependencies>
        <dependency>
            <groupId>com.fisglobal.xpress.atom</groupId>
            <artifactId>CustomerCorrespondenceCommonClasses</artifactId>
            <version>${CustomerCorrespondenceCommonClasses.version}</version>
        </dependency>
        <dependency>
            <groupId>org.springframework.ws</groupId>
            <artifactId>spring-ws-core</artifactId>
            <version>${spring-ws-core.version}</version>
        </dependency>
        <dependency>
            <groupId>commons-codec</groupId>
            <artifactId>commons-codec</artifactId>
            <version>${commons-codec.version}</version>
        </dependency>
        <dependency>
            <groupId>log4j</groupId>
            <artifactId>log4j</artifactId>
            <version>${log4j.version}</version>
        </dependency>
        <dependency>
            <groupId>com.fisglobal.ec.core.base</groupId>
            <artifactId>BaseServerClasses</artifactId>
            <version>${ECCoreBase.version}</version>
        </dependency>
        <dependency>
            <groupId>com.fisglobal.emea</groupId>
            <artifactId>httpclientutility</artifactId>
            <version>1.0-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>com.fisglobal.xpress.emea</groupId>
            <artifactId>testHelpers</artifactId>
            <version>${xpress.testhelper.version}</version>
            <scope>test</scope>
            <exclusions>
                <exclusion>
                    <groupId>com.sun.xml</groupId>
                    <artifactId>jaxb-impl</artifactId>
                </exclusion>
                <exclusion>
                    <groupId>org.apache.ws.commons.axiom</groupId>
                    <artifactId>axiom-api</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>org.jdom</groupId>
            <artifactId>jdom</artifactId>
            <version>${jdom.version}</version>
        </dependency>
        <dependency>
            <groupId>org.apache.ws.commons.axiom</groupId>
            <artifactId>axiom-api</artifactId>
            <version>1.2.12</version>
        </dependency>
        <dependency>
            <groupId>org.testng</groupId>
            <artifactId>testng</artifactId>
            <version>${testng.version}</version>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>org.springframework</groupId>
            <artifactId>spring-test</artifactId>
            <version>${spring-test.version}</version>
            <type>jar</type>
            <scope>test</scope>
        </dependency>
    </dependencies>
</project>
