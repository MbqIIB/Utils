<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
			http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="notificationService" class="com.fisglobal.xpress.emea.notification.impl.NotificationServiceImpl">
        <property name="globalBlackOutTime" ref="globalBlackOutTime"/>
    </bean>

    <bean id="connectionFactory" class="org.springframework.jms.connection.UserCredentialsConnectionFactoryAdapter">
        <property name="targetConnectionFactory" ref="tibcoConnectionFactory"/>
        <property name="username" value="admin"/>
        <property name="password" value=""/>
    </bean>
    <bean id="tibcoConnectionFactory" class="com.tibco.tibjms.TibjmsConnectionFactory">
        <property name="serverUrl" value="tcp://10.166.129.74:7222"/>
        <property name="userName" value="admin"/>
        <property name="userPassword" value=""/>
    </bean>

    <bean id="notificationProcess" class="com.tibco.tibjms.TibjmsQueue">
        <constructor-arg value="notificationProcess"/>
    </bean>

    <bean id="notificationQueue" class="com.tibco.tibjms.TibjmsQueue">
        <constructor-arg value="notificationQueue"/>
    </bean>

    <import resource="mblox-messaging-context.xml"/>

    <bean id="globalBlackOutTime" class="com.fisglobal.xpress.emea.notification.BlackOutTime">
        <constructor-arg index="0" value="21:00"/>
        <constructor-arg index="1" value="23:30"/>
    </bean>

</beans>