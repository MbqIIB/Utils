<?xml version="1.0" encoding="UTF-8"?>
<NotificationRequest Version="3.5">
    <NotificationHeader>
        <PartnerName>${partnerName}</PartnerName>
        <PartnerPassword>${partnerPassword}</PartnerPassword>
    </NotificationHeader>
    <NotificationList BatchID="${messageId}">
        <Notification SequenceNumber="1" MessageType="SMS" Format="UTF8">
            <Message>${message}</Message>
            <Profile>${profileId}</Profile>
            <SenderID Type="Alpha">${senderId}</SenderID>
            <Subscriber>
                <SubscriberNumber>${subscriberNumber}</SubscriberNumber>
            </Subscriber>
            <ContentType>-1</ContentType>
        </Notification>
    </NotificationList>
</NotificationRequest>